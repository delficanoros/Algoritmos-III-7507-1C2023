!classDefinition: #OOStack category: 'Stack-Exercise'!
Object subclass: #OOStack
	instanceVariableNames: 'stack'
	classVariableNames: ''
	poolDictionaries: ''
	category: 'Stack-Exercise'!

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:16:33'!
errorStackVacio
    self error: self class stackEmptyErrorDescription.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:05:50'!
initializeStack
	stack := OrderedCollection new.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:00:03'!
isEmpty
	^stack isEmpty.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:13:56'!
noSePuedeAñadirUnElementoVacio
	 ^self error: self class elementoEstaVacioError.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:15:28'!
pop
    stack isEmpty ifTrue:[self errorStackVacio].
    ^stack removeLast.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:13:04'!
push: unElemento
    unElemento isEmpty ifTrue: [self noSePuedeAñadirUnElementoVacio].
    ^stack add: unElemento.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:21:00'!
size 
	^stack size.! !

!OOStack methodsFor: 'as yet unclassified' stamp: 'Fm 5/4/2023 19:17:43'!
top 
	stack isEmpty ifTrue:[self errorStackVacio].

    	^stack at: stack size.! !

